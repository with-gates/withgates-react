{
  "name": "@withgates/node",
  "version": "1.0.0-alpha.3",
  "description": "Node.js SDK for WithGates",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "types/index.d.ts",
  "type": "module",
  "license": "MIT",
  "files": [
    "dist",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/with-gates/withgates-js.git",
    "directory": "packages/node"
  },
  "homepage": "https://github.com/with-gates/withgates-js/tree/main/packages/node",
  "author": "WithGates",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "watch": "tsup src/index.ts --format esm,cjs --dts --watch",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "publish": "npm run build && npm publish --access public"
  },
  "dependencies": {
    "@withgates/core-js": "*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.2.0",
    "prettier-plugin-organize-imports": "^3.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.3"
  }
}
